# schema.graphqls

type History {
    id: ID!
    paciente: String!
    medico: String!
    enfermeiro: String!
    dataHora: String!
}

extend type Query {
    getPatientHistory(paciente: String!): [History]
}

extend type Mutation {
    addHistory(paciente: String!, medico: String!, enfermeiro: String!, dataHora: String!): History
}